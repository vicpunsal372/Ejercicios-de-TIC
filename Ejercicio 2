#include <stdio.h>

int main()
{
    int fecha;
    printf("Introduzca la fecha: ");
    scanf("%d", &fecha);
    printf("\n");
       
    if(fecha % 4 == 0 && !(fecha%100==0 && !fecha%400!=0)){
    printf("El año es bisiesto");
    }
   
    else{
    printf("El año no es bisiesto");
    }
   

    return 0;
}
